{
  "name": "ih-systems",
  "version": "5.0.10",
  "private": true,
  "main": "electron/main.js",
  "productName": "ih-systems",
  "homepage": "./",
  "build": {
    "win": {
      "icon": "build/icon.png"
    },
    "mac": {
      "icon": "build/icon.png"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "github",
      "repo": "ih-v5",
      "owner": "intrahouseio"
    }
  },
  "dependencies": {
    "@material-ui/core": "4.11.3",
    "@material-ui/data-grid": "4.0.0-alpha.30",
    "@material-ui/icons": "4.11.2",
    "@rh389/shortid": "^2.2.8",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "events": "^3.3.0",
    "form-data": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "simple-peer": "^9.11.0",
    "web-vitals": "^1.0.1",
    "wrtc": "^0.4.7",
    "node-fetch": "2.6.1",
    "systeminformation": "^5.7.7"
  },
  "scripts": {
    "electron": "APP_DEV=true electron .",
    "electron-dev": "ELECTRON_START_URL=http://localhost:3000 electron .",
    "build-electron": "mkdir build/electron && cp -r electron/. build/electron",
    "package": "electron-builder build --mac --linux --win -c.extraMetadata.main=build/electron/main.js --publish never",
    "start": "export BROWSER=none && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^12.0.2",
    "electron-builder": "^22.10.5"
  },
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "author": "",
  "license": "ISC"
}
